<aside style="width: 100%; display: flex; flex-direction: column; gap: 16px;">
  <% const videos = [
    { img: 'rVideo1.png', author: 'James Gouse' },
    { img: 'rVideo2.png', author: 'Marcus Levin' },
    { img: 'rVideo3.png', author: 'Alexis Sears' },
    { img: 'rVideo4.png', author: 'Jesica Lambert' },
    { img: 'rVideo5.png', author: 'Anna White' },
    { img: 'rVideo6.png', author: 'Skylar Dias' },
    { img: 'rVideo7.png', author: 'Alan Cooper' },
    { img: 'rVideo8.png', author: 'James Gouse' },
    { img: 'rVideo2.png', author: 'James Gouse' }
  ]; %>

  <% videos.forEach(v => { %>
    <div style="display: flex; gap: 10px;">
      <div style="position: relative;">
        <img src="/public/img/videoPage/<%= v.img %>" style="width: 168px; height: 94px; object-fit: cover; border-radius: 6px;">
        <span style="position: absolute; bottom: 4px; right: 4px; background-color: rgba(0,0,0,0.8); color: white; font-size: 12px; padding: 1px 4px; border-radius: 3px;">23:45</span>
      </div>
      <div style="font-size: 14px;">
        <strong style="display: block; margin-bottom: 4px;">Ep 6: Living to Serve | SEARCH ON</strong>
        <span style="color: gray;"><%= v.author %> • 1M views • 3 years ago</span>
      </div>
    </div>
  <% }) %>
</aside>